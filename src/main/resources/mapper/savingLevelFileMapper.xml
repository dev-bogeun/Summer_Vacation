<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tikkeul.app.mapper.SavingLevelFileMapper">
    <insert id="insert">
        INSERT INTO SAVINGLEVELFILE
        (ID, SAVINGLEVEL_ID)
        VALUES(#{id}, #{savinglevelId})
    </insert>

    <delete id="delete">
        DELETE FROM SAVINGLEVELFILE
        WHERE SAVINGLEVEL_ID = #{savinglevelId}
    </delete>

</mapper>